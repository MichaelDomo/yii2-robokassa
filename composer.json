{
  "name": "michaeldomo/yii2-robokassa",
  "description": "Yii2 extension for robokassa payment gateway",
  "type": "yii2-extension",
  "keywords": ["yii2", "robokassa"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Michael Domo",
      "email": "domolazov.michael@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/MichaelDomo/yii2-robocassa/issues?state=open",
    "source": "https://github.com/MichaelDomo/yii2-robocassa"
  },
  "require": {
    "yiisoft/yii2": "^2.0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "michaeldomo\\robokassa\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "michaeldomo\\robokassa\\tests\\": "tests/"
    }
  }
}